{
  "expo": {
    "name": "Tattoo Studio",
    "slug": "Tattoo-studio",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_01.png",
    "splash": {
      "image": "./assets/splash_screen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tattoo-studio"
    },
    "android": {
      "package": "com.tattoo_studio"
    }
  }
}